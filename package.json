{
  "name": "wikistats",
  "version": "0.0.1",
  "description": "A front end for Wikimedia project statistics.  This is a new version implemented with Vue and Semantic UI.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "build": "NODE_ENV=production webpack -p --config webpack/prod.config.js",
    "start": "NODE_ENV=development webpack-dev-server --config webpack/dev.config.js",
    "dev": "./node_modules/webpack/bin/webpack.js --config webpack/dev.config.js --progress --watch",
    "server": "python -m SimpleHTTPServer 5000"
  },
  "dependencies": {
    "crossfilter": "^1.3.12",
    "d3": "^4.7.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "pageviews": "^1.3.1",
    "semantic-ui": "^2.2.9",
    "vue": "^2.2.1",
    "vue-router": "^2.2.0",
    "wmui-base": "wikimedia/WikimediaUI-Base"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "css-loader": "^0.28.2",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.6.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-webpack": "^2.0.3",
    "style-loader": "^0.18.1",
    "uglify-es-webpack-plugin": "0.0.2",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.0.4",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milimetric/wikistats-prototype.git"
  },
  "keywords": [
    "Mediawiki",
